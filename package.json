    "@wordpress/scripts": "^29.0.0",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^12.0.2",
    "raw-loader": "^4.0.2",
    "gulp": "^5.0.0",
    "gulp-freemius-deploy": "^1.0.11",
    "gulp-zip": "^6.0.0"
  }
}
